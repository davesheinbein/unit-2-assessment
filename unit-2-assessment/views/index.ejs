<!DOCTYPE html>
<html>

<head>
  <title><%= title %></title>
  <link rel='stylesheet' href='/stylesheets/style.css' />
</head>

<body>
  <h1><%= title %></h1>
  <p>Welcome to <%= title %></p>

  <h1><%# list.list  %> </h1>
  <h1><%# list.item  %> </h1>

  <ul>
    <% lists.forEach(function(l) { %>
    <li>
      <form action="/<%= l.id %>?_method=DELETE" class="delete-form" method="POST">
        <button type="submit">X</button>
      </form>
    </li>
    <% }); %>
  </ul>

</body>

</html>